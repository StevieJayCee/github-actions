{
  "ci": {
    "collect": {
      "url": [
        "https://wattpad.com/",
        "https://wattpad.com/getmobile",
        "https://wattpad.com/help/dmca"
      ],
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.8 }],
        "categories:accessibility": ["error", { "minScore": 0.8 }],
        "categories:seo": "off",
        "categories:best-practices": "off"
      }
    }
  }
}
